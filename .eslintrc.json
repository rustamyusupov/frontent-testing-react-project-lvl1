{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "parser": "@babel/eslint-parser",
  "extends": [
    "airbnb-base",
    "plugin:jest/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["@babel", "import", "jest", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0
  }
}